-- Структура таблицы `e7ya_event_log`
--

CREATE TABLE `e7ya_event_log` (
  `id` int(11) NOT NULL,
  `eventid` int(11) DEFAULT 0,
  `createdon` int(11) NOT NULL DEFAULT 0,
  `type` int(11) NOT NULL DEFAULT 1 COMMENT '1- information, 2 - warning, 3- error',
