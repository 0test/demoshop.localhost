-- Ограничения внешнего ключа таблицы `f1nj_sp_votes`
--
ALTER TABLE `f1nj_sp_votes`
  ADD CONSTRAINT `sp_votes_ibfk_1` FOREIGN KEY (`vote_poll`) REFERENCES `f1nj_sp_polls` (`poll_id`) ON DELETE CASCADE;
--
-- База данных: `efrance`
--
CREATE DATABASE IF NOT EXISTS `efrance` DEFAULT CHARACTER SET utf8mb4 COLLATE utf8mb4_unicode_ci;
USE `efrance`;

