-- --------------------------------------------------------

--
-- Структура таблицы `dsfh_site_content_categories`
--

CREATE TABLE `dsfh_site_content_categories` (
  `doc` int(10) NOT NULL,
  `category` int(10) NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

-- --------------------------------------------------------

--
-- Структура таблицы `dsfh_site_content_closure`
--

CREATE TABLE `dsfh_site_content_closure` (
  `closure_id` int(10) UNSIGNED NOT NULL,
  `ancestor` int(10) UNSIGNED NOT NULL,
  `descendant` int(10) UNSIGNED NOT NULL,
  `depth` int(10) UNSIGNED NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

--
-- Дамп данных таблицы `dsfh_site_content_closure`
--

INSERT INTO `dsfh_site_content_closure` (`closure_id`, `ancestor`, `descendant`, `depth`) VALUES
(1, 1, 1, 0),
(2, 2, 2, 0),
(3, 100, 100, 0),
(4, 2, 2, 0);

