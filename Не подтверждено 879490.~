--
ALTER TABLE `adkq_commerce_order_payments`
  ADD PRIMARY KEY (`id`),
  ADD KEY `order_id` (`order_id`),
  ADD KEY `hash` (`hash`),
  ADD KEY `original_order_id` (`original_order_id`),
  ADD KEY `original_order_id_2` (`original_order_id`);

