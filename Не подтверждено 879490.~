--
-- Ограничения внешнего ключа таблицы `f1nj_sp_log`
--
ALTER TABLE `f1nj_sp_log`
  ADD CONSTRAINT `sp_log_ibfk_1` FOREIGN KEY (`poll`) REFERENCES `f1nj_sp_polls` (`poll_id`) ON DELETE CASCADE;

