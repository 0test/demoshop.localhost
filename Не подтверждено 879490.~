  `id` int(10) NOT NULL,
  `docid` int(10) NOT NULL,
  `table` varchar(255) NOT NULL,
  `pagetitle` varchar(255) NOT NULL,
  `content_with_tv` mediumtext NOT NULL,
  `content_with_tv_index` mediumtext NOT NULL
) ENGINE=MyISAM DEFAULT CHARSET=utf8mb3;

